        self.btnFrequenceOnePerDay.clicked.connect(self.checkBtnOnePerDay)
        self.btnFrequenceTwoPerDay.clicked.connect(self.checkBtnTwoPerDay)
        self.btnFrequenceThreePerDay.clicked.connect(self.checkBtnThreePerDay)

    # 口服
    def checkBtnEatByMouth(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnEatByMouth.text())
        self.appendFenhao()

    # 外用
    def checkBtnUseByExternal(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnUseByExternal.text())
        self.appendFenhao()

    # 饭前
    def checkBtnBeforeEating(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnBeforeEating.text())
        self.appendFenhao()

    def checkBtnAfterEating(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnAfterEating.text())
        self.appendFenhao()

    def connectEatingMethods(self):
        self.btnEatByMouth.clicked.connect(self.checkBtnEatByMouth)
        self.btnUseByExternal.clicked.connect(self.checkBtnUseByExternal)
        self.btnBeforeEating.clicked.connect(self.checkBtnBeforeEating)
        self.btnAfterEating.clicked.connect(self.checkBtnAfterEating)

    def checkBtnNoHormone(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnNoHormone.text())
        self.appendFenhao()

    def checkBtnNoPungent(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnNoPungent.text())
        self.appendFenhao()

    def checkBtnNoTired(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnNoTired.text())
        self.appendFenhao()

    def checkBtnNoSmokeAndDrink(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnNoSmokeAndDrink.text())
        self.appendFenhao()


    def connectWarningThings(self):
        self.btnNoHormone.clicked.connect(self.checkBtnNoHormone)
        self.btnNoPungent.clicked.connect(self.checkBtnNoPungent)
        self.btnNoTired.clicked.connect(self.checkBtnNoTired)
        self.btnNoSmokeAndDrink.clicked.connect(self.checkBtnNoSmokeAndDrink)

    def connectFunction(self):
        # 药物连接
        self.connectMedicals()
        # 功能按钮连接
        self.connectFunctionButtons()
        # 剂量按钮连接
        self.connectDoses()
        # 服用频率按钮连接
        self.connectFrequences()
        # 服用方式
        self.connectEatingMethods()
        # 注意事项
        self.connectWarningThings()

if __name__ == '__main__':
    app = QApplication(sys.argv)
    MainWindow = QMainWindow()
    ui = Ui_MainWindow()
    ui.setupUi(MainWindow)
    MainWindow.showFullScreen()
    sys.exit(app.exec_())
