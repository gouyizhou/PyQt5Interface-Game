        self.medicalTxt.insertPlainText(self.btnDoseThreeBox.text())
        self.appendFenhao()

    def connectDoses(self):
        self.btnDoseOneBox.clicked.connect(self.checkBtnOneBox)
        self.btnDoseTwoBox.clicked.connect(self.checkBtnTwoBox)
        self.btnDoseThreeBox.clicked.connect(self.checkBtnThreeBox)

