    def saveTxt(self):
        # 处方内容
        try:
            str = self.medicalTxt.toPlainText()
            # print(str)
            # 处方名字
            title = self.getTitle()+'.txt'
            with open (title, 'w+') as f:
                f.write(str)
            self.medicalTxt.clear()
            # self.medicalTxt.append('保存成功！处方文件名为:{0}'.format(title))
            self.messageDialog(titles = '保存成功！处方文件名为:{0}'.format(title))
        except:
            self.messageDialog(titles = '保存失败！')
            # self.medicalTxt.append()

    def messageDialog(self, titles):
        msg_box = QMessageBox(QMessageBox.Warning, 'Finish', titles)
        msg_box.exec_()

    def appendColon(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(':')

    def appendFenhao(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText('；')

    def exitSystem(self):
        app.quit()

    def clearMedicalTxt(self):
        self.medicalTxt.clear()
    def getTitle(self):
        str = time.localtime()
        title = str.tm_year.__str__() + "-" + str.tm_mon.__str__() + "-" + str.tm_mday.__str__() \
                + "-" + str.tm_hour.__str__() + "-" + str.tm_min.__str__() + "-处方药"
        return title

    def connectMedicals(self):
        # 盐酸非索非那定片
        self.btnFexofenadine_Hydrochloride_Tablets.clicked.connect(self.checkBtnFexofenadine_Hydrochloride_Tablets)
        # 盐酸奥洛他定片
        self.btnOlopatadine_Hydrochloride_Tablets.clicked.connect(self.checkBtnOlopatadine_Hydrochloride_Tablets)
        # 富马酸氯马斯汀
        self.btnClemastine_fumarate.clicked.connect(self.checkBtnClemastine_fumarate)
        # 咪唑斯汀缓释片
        self.btnMizolastine_Sustained_Release_Tablets.clicked.connect(
            self.checkBtnMizolastine_Sustained_Release_Tablets)
        # 地氯雷他定片
        self.btnDesloratadine_tablets.clicked.connect(self.checkBtnDesloratadine_tablets)
        # 地氯雷他定片混悬剂
        self.btnDesloratadine_dry_suspension.clicked.connect(self.checkBtnDesloratadine_dry_suspension)
        # 泛昔洛韦片
        self.btnFamciclovir_tablets.clicked.connect(self.checkBtnFamciclovir_tablets)
        # 氟康唑胶囊
        self.btnFluconazole_capsules.clicked.connect(self.checkBtnFluconazole_capsules)
        # 伊曲康唑缓释胶囊
        self.btnItraconazole_sustained_release_capsules.clicked.connect(
            self.checkBtnItraconazole_sustained_release_capsules)
        # 盐酸特比萘芬片
        self.btnTerbinafine_hydrochloride_tablets.clicked.connect(self.checkBtnTerbinafine_hydrochloride_tablets)
        # 丹参酮胶囊
        self.btnTanshinone_capsule.clicked.connect(self.checkBtnTanshinone_capsule)
        # 阿维A胶囊
        self.btnAcitretin_capsules.clicked.connect(self.checkBtnAcitretin_capsules)
        # 异维A酸胶丸
        self.btnIsotretinoin_Soft_Capsules.clicked.connect(self.checkBtnIsotretinoin_Soft_Capsules)
        # 美诺平颗粒
        self.btnMenoping_granules.clicked.connect(self.checkBtnMenoping_granules)
        # 二丁胶囊
        self.btnErding_capsule.clicked.connect(self.checkBtnErding_capsule)
        # 复方环磷酰胺片
        self.btnCompound_Cyclophosphamide_Tablets.clicked.connect(self.checkBtnCompound_Cyclophosphamide_Tablets)
        # 艾洛松
        self.Aloson.clicked.connect(self.checkBtnAloson)
        # 维胺脂胶囊
        self.btnWei_An_Zhi_Capsule.clicked.connect(self.checkBtnWei_An_Zhi_Capsule)

    def connectFunctionButtons(self):
        # 开具处方
        self.btnGetMedicalList.clicked.connect(self.saveTxt)
        # 清空处方
        self.btnClearMedical.clicked.connect(self.clearMedicalTxt)
        # 退出
        self.btnExit.clicked.connect(self.exitSystem)

    # 服用剂量
    def checkBtnOneBox(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnDoseOneBox.text())
        self.appendFenhao()

    def checkBtnTwoBox(self):
        self.medicalTxt.moveCursor(QTextCursor.End)
        self.medicalTxt.insertPlainText(self.btnDoseTwoBox.text())
